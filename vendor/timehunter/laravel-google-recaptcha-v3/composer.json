{
  "name": "timehunter/laravel-google-recaptcha-v3",
  "description": "Laravel Package for google reCAPTCHA v3",
  "license": "MIT",
  "authors": [
    {
      "name": "Ryan Deng",
      "email": "ryandadeng@gmail.com",
      "homepage": "https://ryandeng.com"
    }
  ],
  "homepage": "https://github.com/RyanDaDeng/laravel-google-recaptcha-v3",
  "keywords": [
    "laravel recaptcha",
    "googlerecaptcha",
    "google recaptcha",
    "google recaptcha v3",
    "recaptcha v3",
    "laravel google recaptcha v3",
    "laravel google recaptcha",
    "recaptcha",
    "recaptcha v3 laravel",
    "laravel recaptcha v3",
    "laravel"
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "TimeHunter\\LaravelGoogleReCaptchaV3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TimeHunter\\LaravelGoogleReCaptchaV3\\Tests\\": "tests"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "TimeHunter\\LaravelGoogleReCaptchaV3\\Providers\\GoogleReCaptchaV3ServiceProvider"
      ],
      "aliases": {
        "GoogleReCaptchaV3": "TimeHunter\\LaravelGoogleReCaptchaV3\\Facades\\GoogleReCaptchaV3"
      }
    },
    "branch-alias": {
      "dev-master": "3.0.*-dev"
    }
  }
}
